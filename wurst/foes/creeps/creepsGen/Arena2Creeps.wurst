package Arena2Creeps
import UnitIds
import Wave

public constant ARENA3_WAVE_1 = compiletime(wave_1())

function wave_1() returns Wave
  return new WaveBuilder()
  ..expBounty(900)
  ..goldBounty(150)

  ..creep(UnitIds.footman)
  ..hp(9600)
  ..damage(288)
  ..amount(18)
  ..scale(1.25)

  ..creep(UnitIds.knight)
  ..hp(13450)
  ..damage(228)
  ..attackrate(1.0)
  ..amount(4)

  .build()
