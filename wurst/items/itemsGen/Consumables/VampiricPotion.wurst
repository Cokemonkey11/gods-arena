package VampiricPotion
import ItemObjEditing
import AbilityObjEditing
import ObjectIdGenerator
import ObjectIds

public constant VAMPIRIC_POTION_ID = compiletime(ITEM_ID_GEN.next())
constant VAMPIRIC_POTION_ABIL_ID = compiletime(ABIL_ID_GEN.next())

@compiletime function genItems()
  new ItemDefinition(VAMPIRIC_POTION_ID, 'vamp')
  ..setAbilities(VAMPIRIC_POTION_ABIL_ID.toRawCode())
