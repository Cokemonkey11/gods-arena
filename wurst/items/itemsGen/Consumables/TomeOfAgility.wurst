package TomeOfAgility
import ItemObjEditing
import AbilityObjEditing
import ObjectIdGenerator
import ObjectIds

public constant TOME_OF_AGILITY_ID = compiletime(ITEM_ID_GEN.next())
constant TOME_OF_AGILITY_ABIL_ID = compiletime(ABIL_ID_GEN.next())

@compiletime function genItems()
  new ItemDefinition(TOME_OF_AGILITY_ID, 'tdex')
  ..setAbilities(TOME_OF_AGILITY_ABIL_ID.toRawCode())
