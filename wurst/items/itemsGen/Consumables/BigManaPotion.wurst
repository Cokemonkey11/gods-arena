package BigManaPotion
import ItemObjEditing
import AbilityObjEditing
import ObjectIdGenerator
import ObjectIds

public constant BIG_MANA_POTION_ID = compiletime(ITEM_ID_GEN.next())
constant BIG_MANA_POTION_ABIL_ID = compiletime(ABIL_ID_GEN.next())

@compiletime function genItems()
  new ItemDefinition(BIG_MANA_POTION_ID, 'pgma')
  ..setAbilities(BIG_MANA_POTION_ABIL_ID.toRawCode())
