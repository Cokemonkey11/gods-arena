package ManaPotion
import ItemObjEditing
import AbilityObjEditing
import ObjectIdGenerator
import ObjectIds

public constant MANA_POTION_ID = compiletime(ITEM_ID_GEN.next())
constant MANA_POTION_ABIL_ID = compiletime(ABIL_ID_GEN.next())

@compiletime function genItems()
  new ItemDefinition(MANA_POTION_ID, 'pman')
  ..setAbilities(MANA_POTION_ABIL_ID.toRawCode())
